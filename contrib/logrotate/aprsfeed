/home/omni/prod/aprsfeed/logs/*.log {
        daily
        missingok
        rotate 3
        compress
        delaycompress
        notifempty
        olddir /home/omni/prod/aprsfeed/logs/old
        create 640 omni omni
        sharedscripts
        postrotate
          /bin/kill -HUP `cat /home/omni/prod/aprsfeed/aprsfeed.pid 2>/dev/null` 2>/dev/null || true
        endscript
}
